{
  "name": "justinblocker",
  "version": "1.0.0",
  "description": "js blockchain",
  "main": "index.ts",
  "scripts": {
    "build": "tslint && tsc",
    "start": "tsc && node build/index.js",
    "test": "jest",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "keywords": [
    "blockchain"
  ],
  "author": "Einar Alexander Eymundsson",
  "license": "ISC",
  "dependencies": {
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "@types/crypto-js": "^3.1.41",
    "@types/jest": "^23.1.4",
    "crypto-js": "^3.1.9-1",
    "jest": "^23.3.0",
    "ts-jest": "^23.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|js)"
    ]
  }
}
